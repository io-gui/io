{
  "name": "io-gui-workspace",
  "version": "2.0.0-alpha.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && pnpm -r build",
    "bundle": "pnpm lint && pnpm build && node bundle.js",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:core": "vitest run packages/core",
    "test:colors": "vitest run packages/colors",
    "test:editors": "vitest run packages/editors",
    "test:icons": "vitest run packages/icons",
    "test:inputs": "vitest run packages/inputs",
    "test:layout": "vitest run packages/layout",
    "test:markdown": "vitest run packages/markdown",
    "test:menus": "vitest run packages/menus",
    "test:navigation": "vitest run packages/navigation",
    "test:sliders": "vitest run packages/sliders",
    "test:three": "vitest run packages/three",
    "clean": "rm -rf packages/**/dist",
    "publish": "pnpm bundle && pnpm --filter \"./packages/*\" -r publish --tag alpha",
    "publish:dry": "pnpm bundle && pnpm --filter \"./packages/*\" -r publish --tag alpha --dry-run"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-strip": "^3.0.4",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^25.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/ui": "4.0.16",
    "eslint": "^9.39.2",
    "playwright": "^1.57.0",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
