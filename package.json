{
  "name": "io-gui",
  "version": "1.2.5",
  "description": "IoGui is a simple ui library for the web.",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "files": [
    "src",
    "lib",
    "build",
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint src --fix",
    "docs": "typedoc",
    "build": "yarn clean && yarn lint && tsc",
    "watch": "yarn clean && yarn lint && tsc -w",
    "uglify": "uglifyjs bundle/iogui.js -cmo bundle/iogui.js --comments",
    "bundle": "rm -rf bundle && yarn lint && tsc && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/io-gui/io.git"
  },
  "keywords": [
    "io",
    "gui",
    "iogui",
    "iogui"
  ],
  "author": "Aleksandar (Aki) Rodic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iogui/iogui/issues"
  },
  "homepage": "https://iogui.dev",
  "devDependencies": {
    "@rollup/plugin-strip": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-plugin-html": "^6.2.0",
    "rollup": "^2.70.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.0",
    "typescript": "^4.6.3"
  }
}
