  "sideEffects": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/io-gui/io.git"
  },
  "keywords": [
    "ui",
    "io",
    "gui",
    "iogui",
    "io-core"
  ],
  "author": "Aleksandar (Aki) Rodic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/io-gui/io/issues"
  },
  "homepage": "https://iogui.dev",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.5.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/src/",
    "/dist/",
    "/LICENSE",
    "/README.md"
  ],
  "exports": {
