{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./build",
  },
  "typedocOptions": {
    "hideBreadcrumbs": true,
    "hideGenerator": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeInternal": true,
    "excludeExternals": true,
    "cleanOutputDir": true,
    "githubPages": false,
    "readme": "none",
    "gitRevision": "main",
    "entryPoints": [
      "src/io-gui.ts"
    ],
    "out": "docs/tsdoc"
  },
  "include": [
    "./src"
  ]
}
